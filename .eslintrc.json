{
  "extends": ["plugin:flowtype/recommended", "plugin:react/recommended", "airbnb", "prettier", "prettier/react"],
  "plugins": ["flowtype", "prettier", "react"],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2016,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "react/forbid-prop-types": 0,
    "flowtype/no-types-missing-file-annotation": 0
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "jest": true
  }
}
